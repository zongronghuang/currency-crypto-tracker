    "eslint-plugin-testing-library": "^7.13.4",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "msw": "^2.12.2",
